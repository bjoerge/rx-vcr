{
  "name": "rx-vcr",
  "version": "1.0.1",
  "description": "Record and replay RxJS streams",
  "keywords": [
    "record",
    "replay",
    "rxjs",
    "mock"
  ],
  "main": "dist/rx-vcr.umd.js",
  "module": "dist/rx-vcr.es5.js",
  "typings": "dist/types/rx-vcr.d.ts",
  "author": "Bjørge Næss <bjoerge@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js -w",
    "browser": "parcel browser/index.html",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --check"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.0",
    "@types/tempy": "^0.2.0",
    "husky": "^1.3.1",
    "jest": "^24.4.0",
    "lint-staged": "^8.1.5",
    "parcel": "^1.12.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.19.3",
    "rxjs": "^6.4.0",
    "tempy": "^0.2.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "rxjs": "^6.1.0"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjoerge/rx-vcr.git"
  },
  "bugs": {
    "url": "https://github.com/bjoerge/rx-vcr/issues"
  },
  "homepage": "https://github.com/bjoerge/rx-vcr#readme",
  "runkitExampleFilename": "example.js"
}
